#!/bin/sh
# Created by zijian at 2022-04-01 13:46:40

source $HOME/.env/setting
source $HOME/.env/functions/pathremove.func

OPENSSL3_HOME=$HOME/.local/lib/openssl3

if [[ ${USE_LOCAL_OPENSSL3} -eq 1 ]];
then
	export LD_LIBRARY_PATH=${OPENSSL3_HOME}/lib64:$LD_LIBRARY_PATH
else
	pathremove ${OPENSSL3_HOME} LD_LIBRARY_PATH
fi
