#!/bin/sh
# Created by zijian at 2022-04-01 13:14:39

source $HOME/.env/functions/pathremove.func
source $HOME/.env/functions/ev_op.func

BCC_HOME=/usr/share/bcc

if [[ ${USE_BCCTOOL} -eq 1 ]];
then
	is_in ${BCC_HOME}/tools PATH

	if [[ $? -eq 1 ]];
	then
		export PATH=${BCC_HOME}/tools:$PATH
	fi
else
	pathremove ${BCC_HOME} PATH
fi
