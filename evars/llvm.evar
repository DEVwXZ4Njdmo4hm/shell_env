#!/bin/sh
# Created by zijian at 2022-05-06 18:10:21

source $HOME/.env/functions/pathremove.func
source $HOME/.env/functions/ev_op.func

LLVM_HOME=/usr/lib/llvm/$LLVM_TOOLCHAIN_VER/bin

if [[ ${USE_LLVM} -eq 1 ]];
then
	is_in ${LLVM_HOME} PATH

	if [[ $? -eq 1 ]];
	then
		export PATH=$LLVM_HOME:$PATH
	fi
else
	pathremove ${LLVM_HOME} PATH
fi
