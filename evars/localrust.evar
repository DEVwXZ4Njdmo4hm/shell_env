#!/bin/sh
# Created by zijian at 2022-04-01 13:51:42

source $HOME/.env/setting
source $HOME/.env/functions/pathremove.func
source $HOME/.env/functions/ev_op.func

if [[ ${USE_LOCALRUSTLIB} -eq 1 ]];
then
	if [[ $(is_in ${HOME}/.cargo/bin PATH) -eq 1 ]];
	then
		export PATH=$HOME/.cargo/bin:$PATH
	fi
else
	pathremove $HOME/.cargo/bin PATH
fi
