#!/bin/sh
# Created by zijian at 2022-04-01 13:16:44

source $HOME/.env/functions/pathremove.func

if [[ ${USE_GENTOO} -eq 1 ]];
then
	export PORTAGE="/etc/portage"
	export PUSES="$PORTAGE/package.use"
	export PENV="$PORTAGE/env"
	export PMASK="$PORTAGE/package.mask"
else
	unset PORTAGE
	unset PUSES
	unset PENV
	unset PMASK
fi
