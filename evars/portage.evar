#!/bin/sh
# Created by zijian at 2022-04-01 13:16:44

source $HOME/.env/functions/pathremove.func

if [[ ${USE_GENTOO} -eq 1 ]];
then
	PORTAGE="/etc/portage"
	PUSES="$PORTAGE/package.use"
	PENV="$PORTAGE/env"
	PMASK="$PORTAGE/package.mask"
	MAKECONF="$PORTAGE/make.conf"
else
	unset PORTAGE
	unset PUSES
	unset PENV
	unset PMASK
	unset MAKECONF
fi
