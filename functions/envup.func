#!/bin/sh
# Created by zijian at 2022-03-31 14:17:45

functions envup() {
	source $HOME/.shrc
	local current=$(pwd)
	cd $HOME/.env
	{
		git add .
		local create_time=$(date +"%Y-%m-%d %H:%M:%S") 
		local commit_str="Regular update by envup at ${create_time}"
		git commit -m $commit_str
		/usr/bin/wget -q --spider http://baidu.com
		if [[ $? -eq 0 ]];
		then
			git push origin master
		fi
	}

	cd $current
	return 0
}
